# include <stdio.h>
int main()
{
    int arr[5]={2,3,4,7,8};
    int max=arr[0];
    int min=arr[1];
    for(int i=0;i<5;i++)
    {
        if(max<arr [i])
        {
        max=arr[i];
    }
    else if(min>arr[i])
    {
        min=arr[i];
    }
    }

printf("maximum number %d\n",max);
printf("mnimum number %d\n",max);
}
