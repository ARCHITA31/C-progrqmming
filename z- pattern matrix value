#include <stdio.h>

int main(){

  int a[5][5]={{1,2,3,8,9},{4,5,6,7,7},{7,8,9,6,6},{5,7,6,6,4},{0,8,7,5,7}};

   

  for( int i=0;i<5;i++)

  {

    for(int j=0;j<5;j++)

    {

      if(i==0 || i+j==5-1 || i==5-1)

      {

        printf("%d",a[i][j]);

         

      }

      else

      {

        printf(" ");

      }

    }

   

  printf("\n");

  }

  return 0;

}
