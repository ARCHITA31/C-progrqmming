# include <stdio.h>
int main()
{
    char str[10],word[10],rev[10][30];
    int i=0,j=0,k=0,len1=0,len2=0;
    
    printf("enter the string;\n");
    gets(str);
    
    printf("enter the word to be removed:\n
    ");
    gets(word);
    
    for(int i=0;str[i]!='\0'; i++)
    {
        if(str[i]==' ')
        {
            rev[k][j]='\0';
            k++;
            j==0;
        }
        else
        rev[k][j]=str[i];
        j++;
    }
}
rev[k][j]='\0';

j=0;
for(i=0;i<k+1;i++)
{
    if(rev[i][j]=='\0')
    continue;
    else
    printf("%s",rev[i]);
}
printf("\n");
}
}
