// Online C compiler to run C program online
#include <stdio.h>

int main() {
      int n=5;
      for(int i=0;i<n;i++)
      {
     for(int j=0;j<n;j++)
     
         if(i==0|| j==n-1|| i==n/2||j==0)
          
        printf ("* ");
     
       else
       printf("\n ");
      }
      
       printf("\n ");
      }
    
